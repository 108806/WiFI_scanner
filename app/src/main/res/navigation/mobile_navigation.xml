<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_scan">

    <fragment
        android:id="@+id/navigation_scan"
        android:name="com.wlanscanner.fragments.ScanFragment"
        android:label="SCAN"
        tools:layout="@layout/fragment_scan" />

    <fragment
        android:id="@+id/navigation_database"
        android:name="com.wlanscanner.fragments.DatabaseFragment"
        android:label="DATABASE"
        tools:layout="@layout/fragment_database" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.wlanscanner.fragments.MapFragment"
        android:label="MAP"
        tools:layout="@layout/fragment_map" />

</navigation>
